{
  "name": "docs",
  "version": "1.0.0",
  "main": "js/script.js",
  "scripts": {
    "build": "npm run build:ts",
    "build:css": "tailwindcss -i ./build/input.css -o ./build/output.css --watch",
    "build:css:prod": "tailwindcss -i ./build/input.css -o ./build/output.css --minify && cat ./build/custom.css >> ./build/output.css",
    "build:ts": "tsc && npm run build:css:prod",
    "dev": "npm run tsc:watch & npm run build:css",
    "tsc": "tsc",
    "tsc:watch": "tsc --watch",
    "lint": "eslint js/**/*.ts",
    "lint:fix": "eslint js/**/*.ts --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/node": "^24.3.1",
    "@types/swiper": "^5.4.3",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^9.34.0",
    "eslint-plugin-jsdoc": "^54.3.1",
    "postcss": "^8.4.0",
    "tailwindcss": "^2.2.19",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "swiper": "^11.2.10"
  }
}
